<table class="w-full rounded-lg overflow-hidden mt-5">
    <tr class=" bg-slate-500 text-white">
        <th class="p-2">N1</th>
        <th>Ism</th>
        <th>Familya</th>
        <th>Elektron pochta</th>
        <th>Ish haqi</th>
        <th>Sana</th>
        <th>Harakatlar</th>
    </tr>
    {{#each usersData}}
    <tr class="bg-white even:bg-gray-100">
        <td class="p-3">{{id}}</td>
        <td> {{name}} </td>
        <td> {{surname}} </td>
        <td> {{email}} </td>
        <td> {{salary}} </td>
        <td> {{dateString}} </td>
        <td>
            <div class="flex *:m-0.5">
                <a href="/users/edit/{{originalId}}"
                    class="bg-blue-500 rounded-md py-0.5 px-2 text-slate-200 w-full text-center">Tahrirlash</a>
                <a href="/users/delete/{{originalId}}"
                    class="bg-red-500 rounded-md py-0.5 px-2 text-slate-200 w-full text-center">O'chirish</a>
            </div>
        </td>
    </tr>
    {{/each}}
</table>